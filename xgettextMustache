require_once "lib/xgettextMustache.php";


if (count($argv) < 3) {
    print xgettextMustache::getUsage();
    exit(0);
}
$xt = new xgettextMustache();
$xt->extract();

printf("%s wrote.\n", $xt->getOutputFile());

